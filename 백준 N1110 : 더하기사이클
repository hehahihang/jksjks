import java.util.Scanner;

public class N1110 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		sc.close();

		int cnt=1; 
		int num = N;
		
		while(true)
		{
			int a = num/10;
			int b = num%10;
			int sum = a+b;
			num = b*10 + sum%10;
			
			if(N==num)
				break;
			else
				cnt++;
		}
		System.out.println(cnt);
	}
}
